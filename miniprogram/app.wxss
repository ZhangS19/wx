/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.title-bar {
  width: calc( 100% - 40);
  height: 30px;
  margin-left: 20px;
  margin-right: 20px;
  border-radius: 15px;
  background-color: white;
  border: solid 1px #EBEBEB;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.08);
}
.title-text {
  width: 60px;
  line-height: 30px;
  font-size: 15px;
  margin-left: 30px;

}

.b-block {
  width: calc( 100% - 40);
  margin-left: 20px;
  margin-right: 20px;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.08);
  background-color: white;
  border-radius: 15px 15px 0px 0px;
}

/*tabbar test*/
.tabbar-box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 120rpx;
    box-shadow: 0px -5px 6px rgba(0, 0, 0, 0.815);
  
}

.tabbar-nav {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    height: 100%;
}

.tabbar-icon {
    width: 50rpx;
    height:50rpx;
}

.tabbar-nav:nth-child(2) image { 
    position:relative;
    top:-32rpx;
    width:80rpx;
    height:80rpx;
}
.tabbar-nav:nth-child(2) text {  
    position:relative; 
    top:-32rpx;
}