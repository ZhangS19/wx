/* components/tabbar/tabbar.wxss */
.tabbar-box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 120rpx;
    box-shadow: 0px 1px 6px rgba(0, 0, 0, 0.116);
  
}
.tabbar-nav {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 25rpx;
    height: 100%;
}
.navigator-hover {
  background-color: rgba(255, 0, 0, 0); opacity: 0;
}

.tabbar-icon {
    width: 50rpx;
    height: 50rpx;
}
